{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended",
        "prettier",
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "rules": {
        "prettier/prettier": "error",
        "camelcase": "off",
        "no-console": "off"
    },
    "plugins": [
        "prettier"
    ]
}